{
  "id": null,
  "uid": "forgeops-overview",
  "title": "ForgeOps Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Incident Intake Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(incidents_created_total[1m]))",
          "legendFormat": "incidents/min"
        }
      ],
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8}
    },
    {
      "type": "timeseries",
      "title": "Resolution Time (p95)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(incident_resolution_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8}
    },
    {
      "type": "timeseries",
      "title": "Queue Depth",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "incident_queue_depth",
          "legendFormat": "queue"
        }
      ],
      "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8}
    },
    {
      "type": "timeseries",
      "title": "Processed Outcomes",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(incident_processed_total[1m])) by (outcome)",
          "legendFormat": "{{outcome}}"
        }
      ],
      "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8}
    }
  ],
  "templating": {"list": []},
  "time": {"from": "now-15m", "to": "now"}
}
